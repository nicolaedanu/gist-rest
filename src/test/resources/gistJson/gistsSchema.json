{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "url",
      "forks_url",
      "commits_url",
      "id",
      "node_id",
      "git_pull_url",
      "git_push_url",
      "html_url",
      "files",
      "public",
      "created_at",
      "updated_at",
      "description",
      "comments",
      "user",
      "comments_url",
      "owner",
      "truncated"
    ],
    "properties": {
      "url": {
        "$id": "#/items/properties/url",
        "type": "string",
        "title": "The Url Schema",
        "default": "",
        "examples": [
          "https://api.github.com/gists/aa5a315d61ae9438b18d"
        ],
        "pattern": "^(.*)$"
      },
      "forks_url": {
        "$id": "#/items/properties/forks_url",
        "type": "string",
        "title": "The Forks_url Schema",
        "default": "",
        "examples": [
          "https://api.github.com/gists/aa5a315d61ae9438b18d/forks"
        ],
        "pattern": "^(.*)$"
      },
      "commits_url": {
        "$id": "#/items/properties/commits_url",
        "type": "string",
        "title": "The Commits_url Schema",
        "default": "",
        "examples": [
          "https://api.github.com/gists/aa5a315d61ae9438b18d/commits"
        ],
        "pattern": "^(.*)$"
      },
      "id": {
        "$id": "#/items/properties/id",
        "type": "string",
        "title": "The Id Schema",
        "default": "",
        "examples": [
          "aa5a315d61ae9438b18d"
        ],
        "pattern": "^(.*)$"
      },
      "node_id": {
        "$id": "#/items/properties/node_id",
        "type": "string",
        "title": "The Node_id Schema",
        "default": "",
        "examples": [
          "MDQ6R2lzdGFhNWEzMTVkNjFhZTk0MzhiMThk"
        ],
        "pattern": "^(.*)$"
      },
      "git_pull_url": {
        "$id": "#/items/properties/git_pull_url",
        "type": "string",
        "title": "The Git_pull_url Schema",
        "default": "",
        "examples": [
          "https://gist.github.com/aa5a315d61ae9438b18d.git"
        ],
        "pattern": "^(.*)$"
      },
      "git_push_url": {
        "$id": "#/items/properties/git_push_url",
        "type": "string",
        "title": "The Git_push_url Schema",
        "default": "",
        "examples": [
          "https://gist.github.com/aa5a315d61ae9438b18d.git"
        ],
        "pattern": "^(.*)$"
      },
      "html_url": {
        "$id": "#/items/properties/html_url",
        "type": "string",
        "title": "The Html_url Schema",
        "default": "",
        "examples": [
          "https://gist.github.com/aa5a315d61ae9438b18d"
        ],
        "pattern": "^(.*)$"
      },
      "files": {
        "$id": "#/items/properties/files",
        "type": "object",
        "title": "The Files Schema"
      },
      "public": {
        "$id": "#/items/properties/public",
        "type": "boolean",
        "title": "The Public Schema",
        "default": false,
        "examples": [
          true
        ]
      },
      "created_at": {
        "$id": "#/items/properties/created_at",
        "type": "string",
        "title": "The Created_at Schema",
        "default": "",
        "examples": [
          "2010-04-14T02:15:15Z"
        ],
        "pattern": "^(.*)$"
      },
      "updated_at": {
        "$id": "#/items/properties/updated_at",
        "type": "string",
        "title": "The Updated_at Schema",
        "default": "",
        "examples": [
          "2011-06-20T11:34:15Z"
        ],
        "pattern": "^(.*)$"
      },
      "description": {
        "$id": "#/items/properties/description",
        "type": "string",
        "title": "The Description Schema",
        "default": "",
        "examples": [
          "Hello World Examples"
        ],
        "pattern": "^(.*)$"
      },
      "comments": {
        "$id": "#/items/properties/comments",
        "type": "integer",
        "title": "The Comments Schema",
        "default": 0,
        "examples": [
          0
        ]
      },
      "user": {
        "$id": "#/items/properties/user",
        "type": "null",
        "title": "The User Schema",
        "default": null,
        "examples": [
          null
        ]
      },
      "comments_url": {
        "$id": "#/items/properties/comments_url",
        "type": "string",
        "title": "The Comments_url Schema",
        "default": "",
        "examples": [
          "https://api.github.com/gists/aa5a315d61ae9438b18d/comments/"
        ],
        "pattern": "^(.*)$"
      },
      "owner": {
        "$id": "#/items/properties/owner",
        "type": "object",
        "title": "The Owner Schema",
        "required": [
          "login",
          "id",
          "node_id",
          "avatar_url",
          "gravatar_id",
          "url",
          "html_url",
          "followers_url",
          "following_url",
          "gists_url",
          "starred_url",
          "subscriptions_url",
          "organizations_url",
          "repos_url",
          "events_url",
          "received_events_url",
          "type",
          "site_admin"
        ],
        "properties": {
          "login": {
            "$id": "#/items/properties/owner/properties/login",
            "type": "string",
            "title": "The Login Schema",
            "default": "",
            "examples": [
              "octocat"
            ],
            "pattern": "^(.*)$"
          },
          "id": {
            "$id": "#/items/properties/owner/properties/id",
            "type": "integer",
            "title": "The Id Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "node_id": {
            "$id": "#/items/properties/owner/properties/node_id",
            "type": "string",
            "title": "The Node_id Schema",
            "default": "",
            "examples": [
              "MDQ6VXNlcjE="
            ],
            "pattern": "^(.*)$"
          },
          "avatar_url": {
            "$id": "#/items/properties/owner/properties/avatar_url",
            "type": "string",
            "title": "The Avatar_url Schema",
            "default": "",
            "examples": [
              "https://github.com/images/error/octocat_happy.gif"
            ],
            "pattern": "^(.*)$"
          },
          "gravatar_id": {
            "$id": "#/items/properties/owner/properties/gravatar_id",
            "type": "string",
            "title": "The Gravatar_id Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "url": {
            "$id": "#/items/properties/owner/properties/url",
            "type": "string",
            "title": "The Url Schema",
            "default": "",
            "examples": [
              "https://api.github.com/users/octocat"
            ],
            "pattern": "^(.*)$"
          },
          "html_url": {
            "$id": "#/items/properties/owner/properties/html_url",
            "type": "string",
            "title": "The Html_url Schema",
            "default": "",
            "examples": [
              "https://github.com/octocat"
            ],
            "pattern": "^(.*)$"
          },
          "followers_url": {
            "$id": "#/items/properties/owner/properties/followers_url",
            "type": "string",
            "title": "The Followers_url Schema",
            "default": "",
            "examples": [
              "https://api.github.com/users/octocat/followers"
            ],
            "pattern": "^(.*)$"
          },
          "following_url": {
            "$id": "#/items/properties/owner/properties/following_url",
            "type": "string",
            "title": "The Following_url Schema",
            "default": "",
            "examples": [
              "https://api.github.com/users/octocat/following{/other_user}"
            ],
            "pattern": "^(.*)$"
          },
          "gists_url": {
            "$id": "#/items/properties/owner/properties/gists_url",
            "type": "string",
            "title": "The Gists_url Schema",
            "default": "",
            "examples": [
              "https://api.github.com/users/octocat/gists{/gist_id}"
            ],
            "pattern": "^(.*)$"
          },
          "starred_url": {
            "$id": "#/items/properties/owner/properties/starred_url",
            "type": "string",
            "title": "The Starred_url Schema",
            "default": "",
            "examples": [
              "https://api.github.com/users/octocat/starred{/owner}{/repo}"
            ],
            "pattern": "^(.*)$"
          },
          "subscriptions_url": {
            "$id": "#/items/properties/owner/properties/subscriptions_url",
            "type": "string",
            "title": "The Subscriptions_url Schema",
            "default": "",
            "examples": [
              "https://api.github.com/users/octocat/subscriptions"
            ],
            "pattern": "^(.*)$"
          },
          "organizations_url": {
            "$id": "#/items/properties/owner/properties/organizations_url",
            "type": "string",
            "title": "The Organizations_url Schema",
            "default": "",
            "examples": [
              "https://api.github.com/users/octocat/orgs"
            ],
            "pattern": "^(.*)$"
          },
          "repos_url": {
            "$id": "#/items/properties/owner/properties/repos_url",
            "type": "string",
            "title": "The Repos_url Schema",
            "default": "",
            "examples": [
              "https://api.github.com/users/octocat/repos"
            ],
            "pattern": "^(.*)$"
          },
          "events_url": {
            "$id": "#/items/properties/owner/properties/events_url",
            "type": "string",
            "title": "The Events_url Schema",
            "default": "",
            "examples": [
              "https://api.github.com/users/octocat/events{/privacy}"
            ],
            "pattern": "^(.*)$"
          },
          "received_events_url": {
            "$id": "#/items/properties/owner/properties/received_events_url",
            "type": "string",
            "title": "The Received_events_url Schema",
            "default": "",
            "examples": [
              "https://api.github.com/users/octocat/received_events"
            ],
            "pattern": "^(.*)$"
          },
          "type": {
            "$id": "#/items/properties/owner/properties/type",
            "type": "string",
            "title": "The Type Schema",
            "default": "",
            "examples": [
              "User"
            ],
            "pattern": "^(.*)$"
          },
          "site_admin": {
            "$id": "#/items/properties/owner/properties/site_admin",
            "type": "boolean",
            "title": "The Site_admin Schema",
            "default": false,
            "examples": [
              false
            ]
          }
        }
      },
      "truncated": {
        "$id": "#/items/properties/truncated",
        "type": "boolean",
        "title": "The Truncated Schema",
        "default": false,
        "examples": [
          false
        ]
      }
    }
  }
}